class Solution {

    public ListNode partition(ListNode head, int x) {
        ListNode smaller = new ListNode(0);
        ListNode higher = new ListNode(0);
        ListNode small = smaller;ListNode high = higher;
        while(head!=null){
            if(head.val<x){
                small.next = new ListNode(head.val);
                small = small.next;
            }else{
                high.next = new ListNode(head.val);
                high = high.next;
            }
            head = head.next;
        }
        high.next = null;small.next = higher.next;
        
        return smaller.next;
    }
}