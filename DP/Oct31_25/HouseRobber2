class Solution {
    public int rob(int[] arr) {
        int n = arr.length;
        if(n==1)return arr[0];
        int[] dp = new int[n+1];
        int[] dp1 = new int[n+1];
        dp[1] = arr[0];
        dp1[1] = arr[1];
        for(int i=2; i<n;i++){
            dp[i] = Math.max(dp[i-1], arr[(i-1)]+dp[i-2]);
        }
        for(int i=2; i<n;i++){
            dp1[i] = Math.max(dp1[i-1], arr[i]+dp1[i-2]);
        }
        return Math.max(dp[n-1], dp1[n-1]);
    }
}